
public class Main {

	public static void main(String[] args) {
		double x = 3.0, y = 4.0, z = -5.0;
		double A, B, C;
		
		A = Math.sqrt(x);
		B = Math.sqrt(y);
		C = Math.sqrt(25.0);
		System.out.printf("A raiz quadrada de %f = %.2f\n", x , A);
		System.out.println("A raiz quadrada de " + y + " = " + B);
		System.out.println("A raiz quadrada de 25.0 = " + C);


	}

}
