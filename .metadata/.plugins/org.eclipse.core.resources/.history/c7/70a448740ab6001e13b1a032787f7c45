import java.util.Scanner;

public class Main {

	
	class Contas{
		public static int soma(int x, int y) {
			return x + y;
		}
	}
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num1, num2;
		num1 = sc.nextInt();
		num2 = sc.nextInt();
		System.out.println("SOMA = " + Contas.soma(num1,num2));
		
		
		sc.close();

	}

}
